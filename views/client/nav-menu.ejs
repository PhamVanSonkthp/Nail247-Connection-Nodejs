<%- include("nav-left"); %>

<script>
    function onMyAcount() {
        if (isDefine(getCookie('_id')) && getCookie('_id') != 'null') {
            window.location.href = '../agency/account'
        } else {
            toggleModalSignInSignUp()
        }
    }
    function onPostAd() {
        if (isDefine(getCookie('_id')) && getCookie('_id') != 'null') {
            window.location.href = '../agency/account?post=show'
        } else {
            toggleModalSignInSignUp()
        }
    }
</script>