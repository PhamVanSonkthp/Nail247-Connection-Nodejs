    <script>
        // bootstrap 3 breakpoints 
        const breakpoint = {
            // extra small screen / phone
            xs: 480,
            // small screen / tablet
            sm: 768,
            // medium screen / desktop
            md: 992,
            // large screen / large desktop
            lg: 1200
        };

        function reloadSlickImages() {
            $('#slick_images').slick({
                autoplay: true,
                autoplaySpeed: 5000,
                draggable: true,
                pauseOnHover: false,
                infinite: true,
                dots: false,
                arrows: true,
                speed: 1000,

                mobileFirst: true,

                slidesToShow: 1,
                slidesToScroll: 1,

                responsive: [{
                    breakpoint: breakpoint.xs,
                    settings: {
                        slidesToShow: 1,
                        slidesToScroll: 1
                    }
                },
                {
                    breakpoint: breakpoint.sm,
                    settings: {
                        slidesToShow: Math.min(2, (post.images.length || 0)),
                        slidesToScroll: Math.min(2, (post.images.length || 0))
                    }
                },
                {
                    breakpoint: breakpoint.md,
                    settings: {
                        slidesToShow: Math.min(3, (post.images.length || 0)),
                        slidesToScroll: Math.min(3, (post.images.length || 0)),
                    }
                },
                {
                    breakpoint: breakpoint.lg,
                    settings: {
                        slidesToShow: Math.min(4, (post.images.length || 0)),
                        slidesToScroll: Math.min(4, (post.images.length || 0)),
                    }
                }
                ]
            });

            $('#slick_images').slick('refresh')
            $('.slick-prev').html('<i class="fas fa-chevron-left fa-lg"></i>')
            $('.slick-next').html('<i class="fas fa-chevron-right fa-lg"></i>')
        }

        function reloadSlick_2() {
            $('#slick_2').slick({
                autoplay: true,
                autoplaySpeed: 500000,
                draggable: true,
                pauseOnHover: false,
                infinite: true,
                dots: false,
                arrows: true,
                speed: 1000,

                mobileFirst: true,

                slidesToShow: 1,
                slidesToScroll: 1,

                responsive: [{
                    breakpoint: breakpoint.xs,
                    settings: {
                        slidesToShow: 2,
                        slidesToScroll: 2
                    }
                },
                {
                    breakpoint: breakpoint.sm,
                    settings: {
                        slidesToShow: 3,
                        slidesToScroll: 3
                    }
                },
                {
                    breakpoint: breakpoint.md,
                    settings: {
                        slidesToShow: 4,
                        slidesToScroll: 4
                    }
                },
                ]
            });
            $('#slick_2').slick('refresh')
            $('.slick-prev').html('<i class="fas fa-chevron-left fa-lg"></i>')
            $('.slick-next').html('<i class="fas fa-chevron-right fa-lg"></i>')
        }
    </script>

    <script>
        let slug = document.location.toString().split('/')[document.location.toString().split('/').length - 1]

        var post

        socket.emit('featured', null, (response) => {
            if (response) {
                $('#slick_2').empty()
                for (let i = 0; i < response.length; i++) {
                    let urlImage
                    if (response[i].type == 1 && isDefine(response[i].images) && response[i].images.length > 0) {
                        urlImage = urlImagePostJobsIcon + response[i].images[0]
                    } else if (response[i].type == 2 && isDefine(response[i].images) && response[i].images.length > 0) {
                        urlImage = urlImagePostSellsSalonsIcon + response[i].images[0]
                    } else if (response[i].type == 3 && isDefine(response[i].images) && response[i].images.length > 0) {
                        urlImage = urlImagePostNailSuppliesIcon + response[i].images[0]
                    } else {
                        urlImage = '../views/admin/dist/images/no-image.png'
                    }
                    let prefix
                    if (response[i].type == 1) {
                        prefix = '../posts-jobs/'
                    } else if (response[i].type == 2) {
                        prefix = '../posts-sell-salons/'
                    } else if (response[i].type == 3) {
                        prefix = '../posts-nail-supplies/'
                    }
                    $('#slick_2').append('<a href="' + (prefix + escapeHtml(response[i].link_slug)) + '"><div class="slide intro-y"> <div class="panel panel-default box"> <img style="border-top-right-radius: 0.5rem;border-top-left-radius: 0.5rem;height: 7rem;width: 100%;object-fit: cover;" src="' + escapeHtml(urlImage) + '" class="img-responsive" /> <div class="panel-body" style="position: relative;"> <h3 class="font-medium ellipsis">' + escapeHtml(response[i].title || 'Unknown') + '</h3> <div class="ellipsis" style="font-size: 0.8rem;">' + escapeHtml(response[i].content || 'Unknown') + '</div> <div class="grid grid-cols-12 mt-2"> <div class="col-span-6 font-medium ellipsis"> <i class="fas fa-phone-alt color-primary"></i> ' + escapeHtml(response[i].phone || 'Unknown') + ' </div> <div class="col-span-6 font-medium ellipsis"> <i class="fas fa-map-marker-alt color-primary"></i> ' + escapeHtml(response[i].country || 'Unknown') + ' </div> </div> <span class="text-white font-medium" style="position: absolute;right: 10px;top: -15px;margin: auto;background: #6bab44;padding: 0.5rem;border-radius: 1rem;font-size: 0.7rem;">' + escapeHtml(response[i].type == 1 ? ('$' + (response[i].cost != 0 ? formatMoney(response[i].cost) : 'contact')) : ('$' + (response[i].price != 0 ? formatMoney(response[i].price) : 'contact'))) + '</span> </div> </div> </div> </a>')
                }
                reloadSlick_2()
            }
        })

        // function position(position) {
        //     latitude = position.coords.latitude
        //     longitude = position.coords.longitude

        //     detailPost(latitude, longitude)
        // }

        function detailPost(latitude, longitude) {
            socket.emit('detail-posts-jobs', { link_slug: slug, latitude: latitude, longitude: longitude }, (response) => {
                if (isDefine(response)) {
                    $('#keyword').val(response.post.country)

                    if (isDefine(response.post.images) && response.post.images.length > 0) {
                        for (let i = 0; i < response.post.images.length; i++) {
                            $('#slick_images').append('<div class="slide"> <div class="panel panel-default" style="cursor: pointer; position: relative;"> <img onclick="slideImage(' + i + ')" src="' + escapeHtml(urlImagePostJobsIcon + response.post.images[i]) + '" class="img-responsive" style="height: 20rem;object-fit: cover;width: 100%;" /> </div> </div>')
                        }
                    }
                    $('#lbl_name_salon').html(escapeHtml(response.post.name_salon))
                    $('#lbl_title').html(escapeHtml(response.post.title))
                    $('#lbl_address').html(escapeHtml(response.post.country))
                    $('#lbl_salary').html(escapeHtml('$' + (response.post.cost != 0 ? formatMoney(response.post.cost) : 'contact')))

                    $('#lbl_content').html((response.post.content))
                    $('#lbl_phone').html(escapeHtml(response.post.phone))
                    $('#lbl_distance').html(escapeHtml(response.post.distance.replace('km', '').trim()))
                    $('#lbl_email').html(escapeHtml(response.post.email))
                    $('#btn_call').attr('href', 'tel:' + escapeHtml(response.post.phone))
                    $('#map').attr('src', 'https://maps.google.com/maps?q=' + response.post.location.coordinates[1] + ', ' + response.post.location.coordinates[0] + '&z=15&output=embed')
                    $('#container_clear_filter').css('display' , 'block')
                    post = response.post
                    if (isDefine(response.post.images) && response.post.images.length > 0) {
                        reloadSlickImages()
                    }

                    if (isDefine(response.related)) {
                        let prefix
                        prefix = '../posts-jobs/'
                        urlImage = urlImagePostJobsIcon
                        for (let i = 0; i < response.related.length; i++) {
                            if (response.related[i].images != null && response.related[i].images.length > 0) {
                                $('#container_related').append('<a href="' + (prefix + escapeHtml(response.related[i].link_slug)) + '" class="intro-x mt-5 zoom-in col-span-12 xl:col-span-6 grid grid-cols-3 box" style="border-radius: 1rem;box-shadow: 0px 1px 6px #bdbdbd"> <div class="col-span-1"> <div  style="background-image: url(' + (urlImage) + escapeHtml(response.related[i].images[0]) + ');height: 100%; border-top-left-radius: 1rem;border-bottom-left-radius: 1rem;" </div></div> </div><div class="col-span-2"> <div class="p-2"> <div class="font-medium ellipsis"> ' + escapeHtml(response.related[i].title) + ' </div> <div class="ellipsis"> ' + escapeHtml(response.related[i].content) + ' </div> </div> <div style="display: flex;"> <span class="text-white font-medium" style="background: #6bab44;padding: 0.5rem;border-top-right-radius: 1rem;border-bottom-right-radius: 1rem;font-size: 0.7rem;">' + escapeHtml('$' + (response.related[i].cost != 0 ? formatMoney(response.related[i].cost) : 'contact')) + ' </span><div class="ml-4" style="display: flex;align-items: center;justify-content: center;"> <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" class="feather feather-git-commit block mx-auto"> <circle cx="12" cy="12" r="4"></circle> <line x1="1.05" y1="12" x2="7" y2="12"></line> <line x1="17.01" y1="12" x2="22.96" y2="12"></line> </svg> <span class="ml-1">' + escapeHtml(response.related[i].distance.replace('km', '').trim()) + ' mile</span> </div> </div> <div class="p-2 grid grid-cols-12" style="gap: 0.5rem;"> <div class="col-span-6 font-medium ellipsis" style="text-overflow: ellipsis;white-space: nowrap;overflow: hidden;"> <i class="fas fa-phone-alt color-primary"></i> ' + escapeHtml(response.related[i].phone) + ' </div> <div class="col-span-6 font-medium ellipsis"> <i class="fas fa-map-marker-alt color-primary"></i> ' + escapeHtml(response.rela