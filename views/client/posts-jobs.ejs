                $('#modal_submit_email').removeClass('show');
                $('body').removeClass('overflow-y-hidden');
            } else {
                $('#modal_submit_email').addClass('show');
            }
        }

        function slideImage(index) {
            var container = [];
            let indexProductPhotoSwipe = 0;
            for (let i = 0; post.images != undefined && i < post.images.length; i++) {
                var item = {
                    src: urlImagePostJobs + post.images[i],
                    w: 0,
                    h: 0,
                };
                container.push(item);

                // if ($('#ImageShow').attr('src').includes(post.images[i])) {
                //     indexProductPhotoSwipe = i;
                // }
                indexProductPhotoSwipe = index;
            }
            if (container.length == 0) return;
            //event.preventDefault();
            var $pswp = $('.pswp')[0],
                options = {
                    index: indexProductPhotoSwipe,
                    bgOpacity: 0.85,
                    preloaderEl: true,
                };
            var gallery = new PhotoSwipe($pswp, PhotoSwipeUI_Default, container, options);

            gallery.listen('gettingData', function (index, item) {
                if (item.w < 1 || item.h < 1) { // unknown size
                    var img = new Image();
                    img.onload = function () { // will get size after load
                        item.w = this.width; // set image width
                        item.h = this.height; // set image height
                        gallery.invalidateCurrItems(); // reinit Items
                        gallery.updateSize(true); // reinit Items
                    }
                    img.src = item.src; // let's download image
                }
            });

            gallery.init();
        }

        function showImage(link) {
            $('#ImageShow').prop("src", link);
        }
    </script>

